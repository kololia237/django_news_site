CREATE TABLE IF NOT EXISTS "app_articles_articles" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "title" varchar(50) NOT NULL, "description" text NOT NULL, "quote" text NOT NULL, "biography" text NOT NULL, "full_name" varchar(50) NOT NULL, "related_books" varchar(50) NOT NULL, "img" varchar(100) NOT NULL, "date_created" datetime NOT NULL, "date_updated" datetime NOT NULL, "views_count" integer NOT NULL, "author_article_id" integer NULL REFERENCES "auth_user" ("id") DEFERRABLE INITIALLY DEFERRED, "category_id" bigint NOT NULL REFERENCES "app_articles_categories" ("id") DEFERRABLE INITIALLY DEFERRED, "connected_book_id" bigint NULL REFERENCES "app_books_books" ("id") DEFERRABLE INITIALLY DEFERRED);
INSERT INTO "app_articles_articles" VALUES(1,'Гаррі Поттер','Гаррі Джеймс Поттер (англ. Harry Potter) — найзнаменитіший студент Гоґвортсу за останні сто років. Перший чарівник, якому вдалося протистояти вбивчому прокляттю Авада Кедавра. Героїчно бився з Лордом Волдемортом і його послідовниками - смертежерами. Єдиний, кому вдавалося залишитися живим після шести поєдинків з Темним Лордом і хто, врешті-решт, переміг його. У Школі Чарів і Чаклунства Гоґвортс його найкращими друзями були чарівники Рон Візлі і Герміона Ґрейнджер. Його найцікавішою особистою прикметою є шрам у вигляді блискавки на лобі, який з''явився, коли Темний Лорд Волдеморт спробував вбити Гаррі ще малям, застосувавши закляття смерті Авада Кедавра.','Одне слово, тебе оберігає твоя здатність любити! - голосно сказав Дамблдор. -А це єдиний оберіг, що може чинити опір тій знадливій силі, якою наділений Волдеморт! Попри всі спокуси, що тебе зваблювали, попри всі твої муки й страждання, ти чистий у серці, так само чистий, як і тоді, коли тобі було одинадцять і ти зазирнув у дзеркало, що відбивало бажання твого серця, і воно тобі показало тільки те, як зупинити Лорда Волдеморта, а не безсмертя чи багатство. Гаррі, чи ти хоч розумієш, що мало хто з чаклунів побачив би в дзеркалі те, що побачив ти?','Походження
Гаррі Поттер — нащадок Певерелів, старовинної магічної сім''ї, до якої має відношення і Том Редл. Втім, кров братів Певерелів, за словами самої Джоан Роулінг, тече в кожному, хто володіє магічними здібностями. Сам Гаррі дізнався про те, що він нащадок Іґнотуса Певерела, коли зрозумів, що плащ-невидимка, що дістався йому від батька, насправді налічує кілька століть і є одним з Смертельних Реліквій. Також відомо, що Том Редл — нащадок Кадмуса Певерела по лінії Ґонтів, що володіли другою реліквією — воскресальним каменем. Таким чином, сам Гаррі і Волдеморт мають спільне давнє коріння.

Гаррі Поттер з батьківського боку споріднений з сімейством Блеків, а через них — з половиною магічних сімейств Англії. З материнської сторони він споріднений з маглівськими сімействами Дурслів та Евансів.','Гаррі Джеймс Поттер','Гаррі Джеймс Поттер','articles_images/2022/07/24/зображення_2022-07-24_131428153.png','2022-07-24 10:14:35.602043','2022-07-24 10:14:35.602043',0,1,1,NULL);
INSERT INTO "app_articles_articles" VALUES(2,'Герміона Ґрейнджер','Герміона Джін Візлі (Ґрейнджер) (ориг. Hermione Jean Granger) (19 вересня 1979) — юна чарівниця з циклу романів про Гаррі Поттера Джоан Роулінг, найкраща подруга Гаррі Поттера та Рона Візлі.','Це ж Герміона. Якщо незрозуміло, йди в бібліотеку.','Герміона народилася 19 вересня 1979 року в родині маґлів, містера і місіс Ґрейнджерів, дантистів за професією. Хоча батьки дивувалися деяким ситуаціям навколо доньки (перші, стихійні, прояви магії), тим не менш, пишалися нею. З дитинства Герміоні прищепили любов до знань, дівчинка росла відповідальною і сумлінною.

Коли Герміоні прийшов лист з школи чарів і чаклунства Гоґвортс, для неї це була велика несподіванка, дізнатися, що вона чарівниця. Придбавши всі необхідні речі для навчання, вона ще влітку проштудіювала всі книжки, як для першого курсу, так і ознайомилася з деякою пізнавальною літературою, і навіть опанувала декілька простих заклинань. До Гоґвортсу Герміона відправлялася з багажем знань про магічну школу, значно ширшим, ніж у деяких напівкровок чи чистокровних дітлахів. Для себе вона визначилася, що хоче потрапити в гуртожиток Ґрифіндор чи Рейвенклов, куди зазвичай приймали сміливих (Ґрифіндор) та спраглих до науки (Рейвенклов) чарівників.','Герміона Джін Візлі','Герміона Джін Візлі','articles_images/2022/07/24/зображення_2022-07-24_131641889.png','2022-07-24 10:16:45.636979','2022-07-24 10:16:45.636979',0,1,1,NULL);
INSERT INTO "app_articles_articles" VALUES(3,'Гоґвортс','Гоґвортська Школа Чарів і Чаклунства (ориг. Hogwarts) — вигадана школа чарівництва із серії романів англійської письменниці Джоан Роулінг «Гаррі Поттер», яка розташована у Великобританії. Гоґвортс є основним місцем дії всіх 7 книг про Гаррі Поттера.','…величезний, доволі страхітливий на вигляд замок, що розвалюється, із хаотичною плутаниною башточок та зубчатих стін. Подібно до будинку Візлів, ця будівля не могла бути побудована маґлами, тому що вона підтримується чарами','Гасло школи: «Draco dormiens nunquam titillandus», що з латини перекладається як «Ніколи не буди сплячого дракона». Джоан Ролінґ пояснює його тим, що хотіла придумати школі не абстрактний, як це трапляється зазвичай, а практичний гасло.

Особливу увагу в Гоґвортсі приділено безпеці. По-перше, будівля захована від очей маґлів. Якщо б комусь із немагічного населення трапилося підійти до воріт школи, то він не побачив би нічого окрім розвалин старої будівлі та таблички «Не входити» («Keep out»).','Гоґвортська Школа Чарів і Чаклунства','Гоґвортська Школа Чарів і Чаклунства','articles_images/2022/07/24/зображення_2022-07-24_131819252.png','2022-07-24 10:18:32.618307','2022-07-24 10:18:32.618307',0,2,2,NULL);
INSERT INTO "app_articles_articles" VALUES(4,'Гоґвортський експрес','Гоґвортський експрес (Англ. Hogwarts Express) - спеціальний поїзд, червоний паровоз, призначений для доставки студентів Гогвортсу до місця навчання і назад. Іноді «Гоґвортський експрес» доставляє до місця роботи нових викладачів','','Наскільки ми знаємо з попередніх історичних джерел, а також різних гравюр, перш студенти Гогвортсу прибували в школу різними способами. Наприклад, деякі прилітали на мітлах (мінусом цього способу є складність перевозу валіз і клітин з тваринами), інші приїжджали на зачарованих возах, а пізніше і в каретах; деякі намагалися роз''явився (найчастіше це закінчувалося трагічно, адже замок і його землі завжди були захищені антиявленими чарами), інші ж приїздили на різних магічних істот.','Гоґвортський експрес','Гоґвортський експрес','articles_images/2022/07/24/зображення_2022-07-24_132026696.png','2022-07-24 10:20:34.687079','2022-07-24 10:20:34.687079',0,2,2,NULL);
